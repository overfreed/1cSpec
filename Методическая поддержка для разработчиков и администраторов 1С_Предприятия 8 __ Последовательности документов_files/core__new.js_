				if(selection.start !== selection.end)
				{
					if(selection.start < selection.end)
					{
						var selectionStart = selection.start;
						var selectionEnd = selection.end;
					}
					else
					{
						var selectionStart = selection.end;
						var selectionEnd = selection.start;
					}
					
