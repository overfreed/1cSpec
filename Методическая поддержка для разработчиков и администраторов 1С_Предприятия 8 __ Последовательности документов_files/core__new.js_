				target = SSP.Form.getInput(target);
				
				if(target.type === 'checkbox')
				{
					if(!event.emulated)
					{
						event.emulated = true;
						target.checked = !target.checked;
					}
				}
				else if(target.type === 'radio' && !target.checked)
					target.checked = true;
			}
			
			return target;
		}
