; /* Start:/bitrix/components/infostart/messenger/templates/min/script.js*/
function ShowRightMenu() {
	$("html").css({"overflow": "hidden"});
	$("#inf-right-menu").show().children(".inf-right-menu-wrap").css({"right": "-300px"});
	$("#inf-right-menu").children(".inf-right-menu-wrap").animate({"right": "0px"}, 400);
	if ($("#inf-right-menu-mess-wrap #NotifyBubble").length < 1) {
		$("#inf-right-menu-mess-wrap").html($("#NotifyBubble"));
	}
}


function get_next_messages(){

	if($('.TAB_MESSAGE').css('display') == 'block'){
		type = 'message';
		obj_li = $('.TAB_MESSAGE li');
		obj = $('.TAB_MESSAGE');
	}
	else{
		type = 'notify';
		obj_li = $('.TAB_NOTIFY li');
		obj = $('.TAB_NOTIFY');
	}

	last_id = '';
	obj_li.each(function(i,elem) {
		last_id = $(elem).data('id');
	});

	$.get( "/bitrix/ajax/im_notify.php?type="+type+"&lid="+last_id, function( data ) {
		html = obj.html();
		html = html + data;
		obj.html( html );
	});
}


$( document ).ready(function() {
	$(".inf-right-menu-close, .inf-right-menu-overlay").click(function () {
		$("html").css({"overflow":""});
		$("#inf-right-menu").children(".inf-right-menu-wrap").animate({"right":"-300px"},400,function () {
			$("#inf-right-menu").hide();
			$("#NotifyBubble").fadeOut(0).removeClass('active');
		});
	});

	$(document).keydown(function (e) {
		if (e.ctrlKey && e.keyCode == 32 && window.getSelection().toString().length > 3) {
			window.location.href = 'https://infostart.ru/profile/support/0/?TITLE=Опечатка на странице '+window.location.href+'&MESSAGE=Текст с опечаткой: '+window.getSelection().toString();
		}
	});
	$("#MessageCounter").on('click', function (){
		ShowRightMenu();
		if ($("#NotifyBubble").is(':visible')){
			$("#NotifyBubble").fadeOut(0).removeClass('active');
		}else{
			$.getJSON( "/bitrix/ajax/im_notify.php?type=count", function( data ) {
					$("#MessageCounter em").remove();
					$("#MessageCounter i").removeClass("bell");
					$("#NotifyBubble #MESSAGE_COUNTER").html(data["MESSAGE_COUNTER"]);
					$("#NotifyBubble #NOTIFY_COUNTER").html(data["NOTIFY_COUNTER"]);
					$("#NotifyBubble").addClass('active').fadeIn(0);
					$("#NotifyBubble #TAB_NOTIFY").click();
					$(".ajax-notifications").height($(".inf-right-menu-wrap").height() - $(".inf-right-menu-items").height() - 95);
			});
		}

		return false;
	});
	$("#NotifyCounter").on('click', function (){
		ShowRightMenu();
		if ($("#NotifyBubble").is(':visible')){
			$("#NotifyBubble").fadeOut(0).removeClass('active');
		}else{
			$.getJSON( "/bitrix/ajax/im_notify.php?type=count", function( data ) {
				$("#MessageCounter em").remove();
				$("#MessageCounter i").removeClass("bell");
				$("#NotifyBubble #MESSAGE_COUNTER").html(data["MESSAGE_COUNTER"]);
				$("#NotifyBubble #NOTIFY_COUNTER").html(data["NOTIFY_COUNTER"]);
				$("#NotifyBubble").addClass('active').fadeIn(0);
				$("#NotifyBubble #TAB_MESSAGE").click();
				$(".ajax-notifications").height($(".inf-right-menu-wrap").height() - $(".inf-right-menu-items").height() - 95);
			});
		}

		return false;
	});
	$("#NotifyBubble").on('click', '.close_btn', function (){
		ShowRightMenu();
		var MID = $(this).parent().attr('data-id');
		var SESSID = $("#sessid").val();
		$.get( "/bitrix/ajax/im_notify.php", {type: 'set_read',mid: MID, sessid: SESSID});
		if ($(this).hasClass('close_btn_notify')){
			var cnt = parseInt($("#NOTIFY_COUNTER").html(), 10) - 1;
			if (cnt >= 0){
				$("#NOTIFY_COUNTER").html(cnt);
			}

			var cnt = parseInt($("#MessageCounter em").html(), 10) - 1;
			if (cnt > 0){
				$("#MessageCounter em").html(cnt);
			}else{
				$("#MessageCounter em").remove();
				$("#MessageCounter i").removeClass("bell");
			}
		}
		if ($(this).hasClass('close_btn_mess')){
			var cnt = parseInt($("#MESSAGE_COUNTER").html(), 10) - 1;
			if (cnt >= 0){
				$("#MESSAGE_COUNTER").html(cnt);
			}

			var cnt = parseInt($("#NotifyCounter em").html(), 10) - 1;
			if (cnt > 0){
				$("#NotifyCounter em").html(cnt);
			}else{
				$("#NotifyCounter em").remove();
			}
		}

		var li = $(this).parent();
		li.animate({
			'margin-left' : '-500px',
		}, 500, function() {
			li.remove();
		});

		return false;
	});
	$("#NotifyBubble").on('click', '.notification-body a', function (){
		// $(this).parents('li').find('.close_btn').click()
	});
	$("#NotifyBubble").on('click', '.notification-body div.msg', function (){
		var href = $(this).attr("href");
		if (href) {
			window.location = href;
		}
	});

	$("#NotifyBubble .btn-group").on('click', 'label', function (){
		var url  = $(this).find('input').attr('id')
		var type = $(this).attr('id')
		var is_loaded = $(this).attr('data-loaded')
		var tab = $("#NotifyBubble .ajax-notifications ." + type)
		$("#NotifyBubble .ajax-notifications > div").hide()
		if (is_loaded == "false"){
			$.get(url, function (data) {
				tab.html(data)
				$('#'+ type).attr("data-loaded", "true")
			})
		}
		tab.show()
	});
});

/* End */
