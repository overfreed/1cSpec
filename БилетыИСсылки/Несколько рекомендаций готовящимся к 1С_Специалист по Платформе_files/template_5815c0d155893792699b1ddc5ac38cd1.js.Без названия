; /* Start:/bitrix/components/infostart/callback/templates/.default/script.js*/
$(document).ready(function () {
	$(document).on("click", ".m-callback-close, .m-callback-close-btn", function () {
        $("#callback-form-modal").modal('toggle');
        return false;
    });
    $(".m-callback-open").click(function () {
        if($("#callback-form-wrap").length > 0){
            $("#callback-form-modal").modal();
        } else {
            $("body").append('<div id="callback-form-wrap"></div>');
        	$("#callback-form-wrap").load("/bitrix/ajax/callback.php", function () {
				$("#callback-form-modal").modal();
			});
		}
        return false;
    });
    $(document).on("submit", "#m-callback", function () {
        $("#m-callback input[name=NUMBER]").css('border-color','');
        if($("#m-callback input[name=NUMBER]").val().length < 10){
            $("#m-callback input[name=NUMBER]").css('border-color','red');
            return false;
        }
		$(".m-callback-success, .m-callback-error").hide();
        $.get('/bitrix/ajax/callback.php?SESSID='+BX.bitrix_sessid()+'&ACTION=call&NUMBER='+$("#m-callback input[name=NUMBER]").val()+'&NAME='+$("#m-callback input[name=NAME]").val(), function(data) {
			var arResult = $.parseJSON(data);
			if (arResult.OK) {
				setTimeout(function () {
                    $("#callback-form-modal").modal('toggle');
				}, 3000);
				$(".m-callback-success").show();
				$("#m-callback").remove();
			}
			if (arResult.ERROR) {
				$(".m-callback-error").show();
			}
		});
        return false;
    });


	$(document).on("submit", "#m-form-request-wrap", function(){
        $("#m-form-request-wrap input, #m-form-request-wrap textarea").css('border-color','');
        formData = new FormData($("#m-form-request-wrap").get(0));
        $.ajax({
            url: '/ajax/send_landing_form.php?ACTION=SEND_FORM&RESPONS=feedback_call&SESSID='+BX.bitrix_sessid(),
            type: 'post',
            contentType: false,
            processData: false,
            data: formData,
            dataType: 'json',
            success: function(arResult){
                if(arResult.ERROR){
                    $("#m-form-request-wrap input[name="+arResult.FIELD+"], #m-form-request-wrap textarea[name="+arResult.FIELD+"]").css('border-color','#e4aeae');
                }else{
					$(".m-callback-success").show();
                    $("#m-form-request-wrap").remove();
                    setTimeout(function () {
                        $("#callback-form-modal").modal('toggle');
                    },3000);

                }
            }
        });
        return false;
    });
});
/* End */
;
; /* Start:/bitrix/components/infostart/support.ticket.edit/templates/chat/script.js*/
window.LIFECHAT_PRODUCT_TYPE = 0;
function OnMessageKeyPress(){
    $('#support-ticket-message').keypress(function() {
    	if (window.SID) {
			var TypingInterval = 10;
			if ((window.LastTypingMessage == undefined) || (window.LastTypingMessage + TypingInterval * 1000 < Date.now())) {
				window.LastTypingMessage = Date.now();
				$.post('/bitrix/ajax/livechat.php', {
					"sessid": BX.bitrix_sessid(),
					"action": "typing",
					"tid": window.SID,
				});
			}
		}
    });
}

function chat_open(){
    $("#support-ticket-create").trigger( "click" );
    return;
}

$(document).ready(function () {

	if ($("#support-ticket-create").length > 0){
		// $("#support-ticket-create").addClass('animated');

		function isScrolledIntoView(elem)
		{
			var docViewTop = $(window).scrollTop();
			var docViewBottom = docViewTop + $(window).height();

			var elemTop = $(elem).offset().top;
			var elemBottom = elemTop + $(elem).height();

			return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
		}

		function ChechkIsFooterVisible() {
			if (isScrolledIntoView('#support-ticket-wrap')){
				$("#support-ticket-create").addClass('animated')
			}else{
				$("#support-ticket-create").removeClass('animated')
			}
		}

		ChechkIsFooterVisible();
		$(window).on('scroll', ChechkIsFooterVisible);

		$(".support-check-var").click(function () {
            if (window.LIFECHAT_PRODUCT_TYPE === 0) {
                var ANSWER = parseInt($("#support-ticket-message").val(), 10);
                $(".support-check-var-error").html('');
                if (isNaN(ANSWER) || ANSWER < 1 || ANSWER > $(".support-check-var-list li").length) {
                    $(".support-check-var-error").html("Не понял вас.<br> Укажите пожалуйста число от 1 до " + $(".support-check-var-list li").length + ".");
                    $("#support-ticket-message").val('').focus();
                    return false;
                }


                $(".support-check-var-list li").each(function () {
                    if ($("#support-ticket-message").val() == ($(this).index()+1)) {
                        window.LIFECHAT_PRODUCT_TYPE = $(this).attr("data-id");
                    }
                });

                if (window.LIFECHAT_PRODUCT_TYPE !== 0) {
                    $("#support-ticket-message").val('').focus();
                    $(".support-check-var-list").prev().html('<b>Ок, понял вас.</b><br> Теперь напишите, какая у вас проблема?');
                    $(".support-check-var-list").remove();

                }

                return false;
            }
        });
	}

    OnMessageKeyPress();
});
$(document).on("click",'#support-ticket-create',function() {
	$("#support-ticket-create").hide();
	$("#support-ticket-created").fadeIn();
    $("#support-ticket-message").focus();
});
$(document).on("click",'#support-ticket-wrap .support-ticket-close',function() {
    $.cookie('support-ticket-min', 'N', { expires: 7, path: '/' });
    /*$.cookie('support-hash', '', { expires: 7, path: '/' });
    $.cookie('support-id', '', { expires: 7, path: '/' });*/
    var ID = $("#support-ticket-message").attr("data-id");

    if (ID > 0) {
        $.post('/bitrix/ajax/livechat.php', {
            "sessid": BX.bitrix_sessid(),
            "ACTION": "AJAX_FORM",
            "set_default": "Y",
            "ID": ID,
            "apply": "Y",
            "CLOSE_WINDOW": "Y"
        }, function (data) {
            $("#support-ticket-wrap").html(data);
        });
    }else{
        $("#support-ticket-created").hide();
        $("#support-ticket-create").fadeIn();

    }



});
$(document).on("click",'#support-ticket-wrap .support-ticket-refresh',function() {
    if(window.SID > 0) {
        $.get("/bitrix/ajax/livechat.php?ACTION=RENEW_TICKET&sessid="+BX.bitrix_sessid(), function (data) {
            $("#support-ticket-wrap").html(data);
            $("#support-ticket-chat").scrollTop(99999999999);
        });
    }
});
$(document).on("click",'#support-ticket-wrap .support-ticket-min',function() {
    $("#support-ticket-wrap .ui-chatbox-content").slideToggle(400,function () {
        if ($("#support-ticket-wrap .ui-chatbox-content:visible").length > 0){
            $.cookie('support-ticket-min', 'N', { expires: 7, path: '/' });
        }else{
            $.cookie('support-ticket-min', 'Y', { expires: 7, path: '/' });
        }
    });

});

$(document).on("mouseover",'#support-ticket-create',function() {
	$("#support-ticket-create .js-livechat-hint").removeClass("hide_hint").addClass("show_hint");
});
$(document).on("mouseout",'#support-ticket-create',function() {
    $("#support-ticket-create .js-livechat-hint").removeClass("show_hint").addClass("hide_hint");
});
$(document).on("change",".fieldAccAgreement:checkbox",function(){1==$(this).prop("checked")?$(this).next("i").css("border-color",""):$(this).next("i").css("border-color","#ff0000")});
function SupportSendMessage() {
    var MessageText = $("#support-ticket-message").val();
	var $parent = $(".ui-chatbox-content");
	if ($parent.find(".fieldAccAgreement:checkbox").length > 0 && $parent.find(".fieldAccAgreement:checkbox").prop("checked") != true) {
		return false;
	}
    if(MessageText.length > 0){
        $('#support-ticket-chat').append('<div class="ui-chatbox-msg ui-chatbox-msg-"><b> Вы : </b><span>' + MessageText + '</span></div>');
        $("#support-ticket-message").val('');
        if ($("#support-ticket-chat .ui-chatbox-msg:last").length > 0) {
			$("#support-ticket-chat").scrollTop($("#support-ticket-chat .ui-chatbox-msg:last").offset().top);
		}
        if ($("#support-ticket-message").attr("data-id") == 0){
            $.cookie('support-ticket-min', 'N', { expires: 7, path: '/' });
            $.post("/bitrix/ajax/livechat.php?_t="+new Date().getTime(),{
				"sessid":BX.bitrix_sessid(),
            	"ACTION":"LIVE_SUPPORT_TICKET",
            	"LIFECHAT_PRODUCT_TYPE":window.LIFECHAT_PRODUCT_TYPE,
                "MESSAGE":MessageText,
            },function (data) {
                $("#support-ticket-wrap").html(data);
                if(window.SUPPORT_HASH) {
                    $.cookie('support-hash', window.SUPPORT_HASH, {expires: 7, path: '/'});
                    $.cookie('support-id', window.SID, {expires: 7, path: '/'});
                }
                OnMessageKeyPress();
            });
        }else{
            $.post('/bitrix/ajax/livechat.php',{
                "sessid":BX.bitrix_sessid(),
                "ACTION":"AJAX_FORM",
                "set_default":"Y",
                "ID":$("#support-ticket-message").attr("data-id"),
                "MESSAGE":MessageText,
                "apply":"Y",
            },function () {
                OnMessageKeyPress();
            });
        }
    }
}

$(document).on("click",'#support-ticket-send',function() {
    SupportSendMessage();
});

$(document).on("keydown",'#support-ticket-message',function(event) {

    if (event.keyCode == 13) {
        if ($("#support-ticket-send").hasClass("support-check-var")){
            $(".support-check-var").click();
            return false;
        }
        SupportSendMessage();
        return false;
    }
});

BX.ready(function(){
    BX.addCustomEvent("onPullEvent", function(module_id,command,params) {
        if(module_id == 'support'){
            if(command == 'disable-chat' && $("#support-ticket-message").length > 0 && parseInt($("#support-ticket-message").attr("data-id")) < 1){
                $("#support-ticket-wrap").remove();
            }

            if(command == 'new-message' && window.SID == params.TID && $("#support-ticket-wrap").length > 0) {
                $.get("/bitrix/ajax/livechat.php?ACTION=RENEW_TICKET&sessid="+BX.bitrix_sessid(),function (data) {
                    $("#support-ticket-wrap .ui-widget-header").html($(data).find(".ui-widget-header").html());
                    $("#support-ticket-chat").html($(data).find("#support-ticket-chat").html());

                    if($(data).find(".alert-msg").length < 1){
                        $("#support-ticket-wrap .alert-msg").remove();
                    }
                    /*$("#support-ticket-wrap").html(data);*/
					if ($("#support-ticket-chat .ui-chatbox-msg:last").length > 0) {
						$("#support-ticket-chat").scrollTop($("#support-ticket-chat .ui-chatbox-msg:last").offset().top);
					}
                    $('#info-message').html('&nbsp;');
                });
            }
            if(command == 'typing' && window.SID == params.TID && window.UID != params.UID && !(window.UID == undefined && params.UID == '') && $("#support-ticket-wrap").length > 0) {
                var TypingInterval = 10;
                $('#info-message').html(params.NAME + ' печатает...');
                setTimeout(function(){
                    $('#info-message').html('&nbsp;');
                }, TypingInterval*1000)
            }
        }
    });
});
/* End */
